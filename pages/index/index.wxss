@font-face {font-family: "iconfont";
  src: url('data:application/x-font-woff;charset=utf-8;base64,') format('woff');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  color: #333;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-caidan:before { content: "\e66b"; }

.icon-xiangji:before { content: "\e642"; }

.icon-luyin:before { content: "\e656"; }

.icon-fanyi:before { content: "\e6ba"; }

.container {
  width: 100%;
  height: 100%;
}

.icon-caidan {
  position: fixed;
  margin: 0 5px;
  line-height: 44px;
}

.tab {
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 48px;
  background: #EFEFEF;
}

.tab icon {
  width: 48px;
  height: 100%;
  line-height: 48px;
  text-align: center;
  font-size: 22px;
}