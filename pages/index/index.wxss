.container {
  width: 100%;
  height: 100%;
}

.tab {
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 48px;
  background: #EFEFEF;
}

@font-face {font-family: "iconfont";
  src: url('data:application/x-font-woff;base64,') format('woff');
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  color: #333;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-menu {
  position: fixed;
  margin: 0 5px;
  line-height: 44px;
}

.icon-ocr, .icon-asr, .icon-mt {
  width: 48px;
  height: 100%;
  line-height: 48px;
  font-size: 22px;
  text-align: center;
}

.icon-menu:before { content: "\e501"; }

.icon-ocr:before { content: "\e642"; }

.icon-asr:before { content: "\e656"; }

.icon-mt:before { content: "\e6ba"; }